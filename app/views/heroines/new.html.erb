<h1> Create New Heroine </h1>
<h1><%=  @put  %></h1>
<%= form_for @heroine do |f| %>
<%= f.label :name%>
<%= f.text_field :name %>
<%= f.label :super_name %>
<%= f.text_field :super_name  %>
<%= f.collection_select :power_id, Power.all, :id, :name, prompt: "select-power"%>
<%= f.submit "Create Heroine" %>
<%end %>
